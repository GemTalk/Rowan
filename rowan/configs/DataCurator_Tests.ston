 RwNestedProjectLoadConfiguration{
	#name : 'DataCurator_Tests',
	#version : '0.1.0',
	#conditionalPackages : {
		[ 'gemstone' ] : {
      'tests': {
				#packageNames : [
					'Rowan-DataCurator-Tests'
					]
				}
			},
	#conditionalPackageMapSpecs : {
		'gemstone' : {
			'allusers' : {
				#packageNameToPlatformPropertiesMap : {
					'Rowan-DataCurator-Tests' : { 'symbolDictName' : 'UserGlobals' }
					}
				},
			'SystemUser' : {
				#packageNameToPlatformPropertiesMap : {
					'Rowan-DataCurator-Tests' : { 'symbolDictName' : 'RowanKernel' }
					}
				}
			}
		},
	#comment : 'Rowan DataCurator tests configuration. Rowan-DataCurator-Tests package will be loaded as part of Tests for normal Rowan load (SystemUser), but DataCurator should be able to load this package as an independent project and then run the tests as well.'
}
