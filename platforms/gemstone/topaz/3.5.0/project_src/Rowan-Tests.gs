	| repos repoRoot testRepoPath loadedProject |
	loadedProject := Rowan image _projectForNonTestProject: 'Rowan'.
	repoRoot :=  loadedProject repositoryRootPath.
	testRepoPath := self _testRowanProjectsSandbox pathString.
