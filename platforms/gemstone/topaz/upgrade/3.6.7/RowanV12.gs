method: RowanClassHierarchyUpdate
writeHierarchyTo: writeStream
	
	classNames do:[:hierarchy |
		hierarchy do:[:name | 
			self writeClass: (self objectInBaseNamed: name) to: writeStream].
		writeStream lf].
	writeStream nextPut: $%; lf.
%

