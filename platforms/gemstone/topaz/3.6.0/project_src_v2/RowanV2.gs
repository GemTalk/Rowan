category: 'initialization'
classmethod: RwModificationFiletreeWriterVisitorV2
initialize
	"self initialize"
    specials := self initializeSpecials
%

category: 'initialization'
classmethod: RwModificationFiletreeWriterVisitorV2
initializeSpecials

    | map |
    map := Dictionary new.
    map
		at: $! put: 'bang';
        at: $+ put: 'plus';
        at: $- put: 'minus';
        at: $= put: 'equals';
        at: $< put: 'less';
        at: $> put: 'more';
        at: $% put: 'percent';
        at: $& put: 'and';
        at: $| put: 'pipe';
        at: $* put: 'star';
        at: $/ put: 'slash';
        at: $\ put: 'backslash';
        at: $~ put: 'tilde';
        at: $? put: 'wat';
        at: $, put: 'comma';
        at: $@ put: 'at'.
    map keys do: [ :key | map at: (map at: key) put: key ].
    ^ map
%

category: 'accessing'
classmethod: RwModificationFiletreeWriterVisitorV2
selectorSpecials

	^ specials ifNil: [ specials := self initializeSpecials ]
%

