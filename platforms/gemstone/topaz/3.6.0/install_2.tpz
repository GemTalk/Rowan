#!/usr/bin/gemstone/topaz
#
# If you are using GsDevKit_home[1] and have stash[2] installed, this topaz 
#	script can be directly executed:
#
#		$ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/install_2.tpz <gsdevkit-stone-name> -lq
#
# If you are not using GsDevKit_home, you can directly run this script as long as
# 	1. $GEMSTONE is defined
# 	2. $GEMSTONE/bin is you path
#	then execute using the following invocation
#
#		$ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/install_1.tpz -lq
#
#	[1] https://github.com/GsDevKit/GsDevKit_home
# [2] https://github.com/dalehenrich/stash
#

	omit pushonly
  iferr 1 stk
  iferr 2 stack
#  iferr 3 exit 1

  set u SystemUser p swordfish
  login

  input $ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/project_src/FileSystemGs.gs
  commit
  input $ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/project_src/STON.gs
  commit
  input $ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/project_src/Cypress.gs
  commit
  input $ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/project_src/Tonel.gs
  commit
  input $ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/project_src/Rowan.gs
  commit
#  input $ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.6.0/project_src/Rowan-Tests.gs
#  commit

	logout

  errorCount

	
