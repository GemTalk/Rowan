directoryPath="$GS_HOME/dev/clients/tode"
${GS_HOME}/bin/private/installGci -f -d $directoryPath -t pharo $vers

newExtent -t -s $GS_HOME/server/stones/$GEMSTONE_NAME/snapshots/extent0.tode.dbf $GEMSTONE_NAME
